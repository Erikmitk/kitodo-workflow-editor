{
  "name": "bpmn-editor-customization-kitodo-production",
  "version": "2.0.1",
  "description": "A customized version of BPMN.io with property panel extension",
  "main": "src/index.js",
  "scripts": {
    "build": "grunt build",
    "dev": "grunt auto-build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://git.slub-dresden.de/sommer/kitodo-workflow-editor"
  },
  "author": {
    "name": "Erik Sommer",
    "url": "https://github.com/erikmitk"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "babel-loader": "^10.0.0",
    "grunt": "^1.6.1",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^3.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-webpack": "^7.0.1",
    "html-webpack-plugin": "^5.6.6",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "load-grunt-tasks": "^5.1.0",
    "raw-loader": "^4.0.2",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "@bpmn-io/properties-panel": "^3.39.0",
    "bpmn-js": "^18.12.0",
    "bpmn-js-properties-panel": "^5.52.1",
    "bpmn-moddle": "^10.0.0",
    "jquery": "^4.0.0"
  },
  "overrides": {
    "minimatch": "^3.1.3",
    "serialize-javascript": "7.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/tests/**/*.test.js"],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(bpmn-js|bpmn-moddle|@bpmn-io|diagram-js)/)"
    ],
    "moduleNameMapper": {
      "\\.bpmn$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.less$": "<rootDir>/tests/__mocks__/fileMock.js"
    }
  }
}
